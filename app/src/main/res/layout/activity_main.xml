<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:background="@color/black"
    android:layout_height="match_parent"
    tools:context=".MainActivity">

    <FrameLayout
        android:id="@+id/fragmentContainer"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_above="@id/bottomNavigation" />




    <com.etebarian.meowbottomnavigation.MeowBottomNavigation
        android:id="@+id/bottomNavigation"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_gravity="bottom"
        android:layout_alignParentBottom="true"
        app:mbn_shadowColor="@color/shadowcolor"
        app:mbn_rippleColor="#2f424242"
        app:mbn_backgroundBottomColor="@color/backgroundbottomcolor"
        app:mbn_circleColor="@color/circelcolor"
        app:mbn_countBackgroundColor="@color/countbackground"
        app:mbn_countTextColor="@color/counttext"
        android:background="@color/white"
        app:mbn_defaultIconColor="@color/defaulticon"
        app:mbn_selectedIconColor="@color/SelectedIcon"/>




</RelativeLayout>
